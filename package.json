{
  "name": "@koalarx/utils",
  "version": "1.0.71",
  "description": "Biblioteca com validadores, conversores e abstrações de algumas problemáticas",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm5/index.js",
  "es2015": "./dist/esm/index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "jest --config ../jestconfig.json",
    "build": "npm run compile && npm run format",
    "compile": "tsc -b ../src/tsconfig.cjs.json ../src/tsconfig.esm.json ../src/tsconfig.esm5.json ../src/tsconfig.types.json",
    "format": "prettier --write \"../src/**/*.ts\" \"../dist/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "copy-default-files": "copy \"./LICENSE\" \"./dist\" && copy \"./README.md\" \"./dist\" && copy \"./package.json\" \"./dist\"",
    "deploy": "npm version patch && npm run copy-default-files && npm publish dist --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igordrangel/koala-utils.git"
  },
  "keywords": [
    "koala",
    "utils",
    "number",
    "string",
    "date",
    "validator",
    "converter",
    "abstract"
  ],
  "author": "Igor D. Rangel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igordrangel/koala-utils/issues"
  },
  "homepage": "https://github.com/igordrangel/koala-utils#readme",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node-fetch": "^2.5.7",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "b64-to-blob": "^1.2.19",
    "date-holidays": "^1.8.1",
    "json-2-csv": "^3.7.8",
    "lodash": "^4.17.19",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1"
  },
  "files": [
    "cjs/**/!(*.tsbuildinfo)",
    "esm/**/!(*.tsbuildinfo)",
    "esm5/**/!(*.tsbuildinfo)",
    "types/**/!(*.tsbuildinfo)",
    "LICENSE",
    "package.json",
    "README.md"
  ]
}
