{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "incremental": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "stripInternal": true,
    "noEmit": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "@koalarx/utils": ["./src/index"],
      "@koalarx/utils/operators/date": ["./src/operators/date/index"],
      "@koalarx/utils/operators/delay": ["./src/operators/delay/index"],
      "@koalarx/utils/operators/number": ["./src/operators/number/index"],
      "@koalarx/utils/operators/string": ["./src/operators/string/index"],
      "@koalarx/utils/enums": ["./src/enums/index"]
    }
  },
  "bazelOptions": {
    "suppressTsconfigOverrideWarnings": true
  }
}
